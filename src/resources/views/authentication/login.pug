extends ../layouts/main.pug
block content
    form.container.mt-5( method='POST')
        .mb-3
            label.form-label(for='#email') Email
            input#email.form-control(type='email' name ='email' placeholder="abc@gmail.com" value = values? values.email : "" )
        .mb-3
            label.form-label(for='#password') Password
            input#password.form-control(type='password' name ='password' value = values ? values.password : "" )
        button.btn.btn-primary(type='submit') Login
        
block more_content